    1  cd ~
    2  git clone https://github.com/mrakitin/dotfiles
    3  git clone https://github.com/NSLS-II/lightsource2-recipes
    4  cd lightsource2-recipes/
    5  git remote add mrakitin https://github.com/mrakitin/lightsource2-recipes
    6  git pull --all
    7  conda create -n py36 python=3.6
    8  source activate py36
    9  conda env list
   10  conda list
   11  history
   12  env | sort
   13  export HISTFILE=~/.bash_history
   14  l
   15  git status

